---
import Section from "../components/Section.astro";
import Layout from "../layouts/Layout.astro";

export let metadata = {
  title: "Projects",
  slug: "projects",
};
---

<Layout title={metadata.title}>
  <main class="container mx-auto p-4 serif">
    <Section slug={metadata.slug} title={metadata.title}>
      <div class="mx-2 my-12">
        <article>
          <h3>photo-app</h3>
        </article>
        <article>
          <h3>ai coding + recommendations</h3>
        </article>
        <article>
          <h3>serverless + edge deploy</h3>
        </article>
      </div>
    </Section>
  </main>
</Layout>
